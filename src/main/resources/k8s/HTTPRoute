apiVersion: gateway.networking.k8s.io/v1
kind: HTTPRoute
metadata:
  name: #(name)
spec:
  parentRefs:
    - name: eg
  rules:
    - backendRefs:
        - group: ""
          kind: Service
          name: #(name)
          port: 11434
          weight: 1
      matches:
        - path:
            type: PathPrefix
            value: /
